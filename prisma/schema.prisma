
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        String   @id @default(uuid())
  username  String
  email     String

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
